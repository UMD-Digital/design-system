{
  "name": "@universityofmaryland/design-system",
  "version": "1.9.10",
  "description": "UMD Design System",
  "license": "MIT",
  "repository": "",
  "private": true,
  "scripts": {
    "clean": "npx lerna run clean",
    "components": "npx lerna run start --scope=@universityofmaryland/web-components-library",
    "components:docs": "npx lerna run docs --scope=@universityofmaryland/web-components-library",
    "elements": "npx lerna run start --scope=@universityofmaryland/web-elements-library",
    "elements:docs": "npx lerna run docs --scope=@universityofmaryland/web-elements-library",
    "feeds": "npx lerna run start --scope=@universityofmaryland/web-feeds-library",
    "feed:docs": "npx lerna run docs --scope=@universityofmaryland/web-feeds-library",
    "styles": "npx lerna run start --scope=@universityofmaryland/web-styles-library",
    "styles:docs": "npx lerna run docs --scope=@universityofmaryland/web-styles-library",
    "styles:test": "npx lerna run test --scope=@universityofmaryland/web-styles-library",
    "utilities": "npx lerna run start --scope=@universityofmaryland/web-utilities-library",
    "utilities:docs": "npx lerna run docs --scope=@universityofmaryland/web-utilities-library",
    "utilities:test": "npx lerna run test --scope=@universityofmaryland/web-utilities-library",
    "icons": "npx lerna run start --scope=@universityofmaryland/web-icons-library",
    "icons:docs": "npx lerna run docs --scope=@universityofmaryland/web-icons-library",
    "icons:test": "npx lerna run test --scope=@universityofmaryland/web-icons-library",
    "model": "npx lerna run start --scope=@universityofmaryland/web-model-library",
    "model:docs": "npx lerna run docs --scope=@universityofmaryland/web-model-library",
    "model:test": "npx lerna run test --scope=@universityofmaryland/web-model-library",
    "test": "npx lerna run test",
    "test:all": "npm run build && npm run generate:mocks && npx lerna run test --stream",
    "test:ci": "npm run build && npm run generate:mocks && npx lerna run test --stream --concurrency=1",
    "theme": "npx lerna run start --scope=@universityofmaryland/theme",
    "start": "npx lerna run start --parallel --stream",
    "build": "npx lerna run build --stream",
    "build:ci": "npx lerna run build --stream --concurrency 1",
    "update-badges": "node scripts/update-badges.js",
    "version:check": "npx lerna changed",
    "version:dry-run": "npx lerna version --no-git-tag-version --no-push --yes",
    "generate:mocks": "node source/generate-mocks.js",
    "package:version": "node scripts/version-bump.js",
    "postpackage:version": "npm run update-badges"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/register": "^7.22.15",
    "@testing-library/jest-dom": "~6.1.5",
    "@types/jest": "^29.5.14",
    "@vitejs/plugin-legacy": "^7.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.0.3",
    "prettier-plugin-twig-melody": "^0.4.6",
    "style-loader": "^3.3.3",
    "to-string-loader": "^1.2.0",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.0",
    "typedoc": "^0.28.1",
    "typedoc-github-wiki-theme": "^2.1.0",
    "typedoc-plugin-markdown": "^4.6.1",
    "typescript": "^5.5.0",
    "url-loader": "^4.1.1",
    "vite": "^7.1.4",
    "vite-plugin-checker": "^0.10.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-shell-plugin-next": "^2.3.2"
  },
  "dependencies": {
    "lerna": "^7.4.1",
    "postcss-discard-duplicates": "^7.0.1",
    "tailwindcss": "^3.3.5"
  }
}
