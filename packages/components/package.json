    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@types/postcss-js": "^4.0.4",
    "jest-dom": "^4.0.0",
    "postcss-js": "^4.0.1",
    "tw-to-css": "^0.0.12"
