{
  "name": "@universityofmaryland/web-components-library",
  "version": "1.12.17",
  "description": "UMD Web Components",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": "",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf build",
    "docs": "npx typedoc --options ./typedoc.json",
