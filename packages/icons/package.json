{
  "name": "@universityofmaryland/web-icons-library",
  "version": "1.0.1",
  "description": "Icon and logo assets for the University of Maryland Design System",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./arrows": {
      "types": "./dist/arrows.d.ts",
      "import": "./dist/arrows.js"
    },
    "./brand": {
      "types": "./dist/brand.d.ts",
      "import": "./dist/brand.js"
    },
    "./calendar": {
      "types": "./dist/calendar.d.ts",
      "import": "./dist/calendar.js"
    },
    "./communication": {
      "types": "./dist/communication.d.ts",
      "import": "./dist/communication.js"
    },
    "./controls": {
      "types": "./dist/controls.d.ts",
      "import": "./dist/controls.js"
    },
    "./files": {
      "types": "./dist/files.d.ts",
      "import": "./dist/files.js"
    },
    "./indicators": {
      "types": "./dist/indicators.d.ts",
      "import": "./dist/indicators.js"
    },
    "./location": {
      "types": "./dist/location.d.ts",
      "import": "./dist/location.js"
    },
    "./logos": {
      "types": "./dist/logos.d.ts",
      "import": "./dist/logos.js"
    },
    "./media": {
      "types": "./dist/media.d.ts",
      "import": "./dist/media.js"
    },
    "./people": {
      "types": "./dist/people.d.ts",
      "import": "./dist/people.js"
    },
    "./search": {
      "types": "./dist/search.d.ts",
      "import": "./dist/search.js"
    },
    "./social": {
      "types": "./dist/social.d.ts",
      "import": "./dist/social.js"
    }
  },
  "license": "MIT",
  "repository": "",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "docs": "npx typedoc --options ./typedoc.json",
    "prebuild": "node ../../scripts/update-badges.js",
    "release": "npm run test && npm run build && npm publish --access public",
    "build": "vite build",
    "dev": "vite build --watch",
    "start": "npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:snapshot": "jest --updateSnapshot"
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^7.2.1",
    "typescript": "^5.8.2",
    "vite": "^7.1.4",
    "vite-plugin-checker": "^0.10.3",
    "vite-plugin-dts": "^4.5.4"
  }
}
