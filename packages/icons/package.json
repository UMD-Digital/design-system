{
  "name": "@universityofmaryland/web-icons-library",
  "version": "1.0.0",
  "description": "Icon and logo assets for the University of Maryland Design System",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./alerts": {
      "types": "./dist/alerts.d.ts",
      "import": "./dist/alerts.mjs",
      "require": "./dist/alerts.js"
    },
    "./brand": {
      "types": "./dist/brand.d.ts",
      "import": "./dist/brand.mjs",
      "require": "./dist/brand.js"
    },
    "./communication": {
      "types": "./dist/communication.d.ts",
      "import": "./dist/communication.mjs",
      "require": "./dist/communication.js"
    },
    "./content": {
      "types": "./dist/content.d.ts",
      "import": "./dist/content.mjs",
      "require": "./dist/content.js"
    },
    "./documents": {
      "types": "./dist/documents.d.ts",
      "import": "./dist/documents.mjs",
      "require": "./dist/documents.js"
    },
    "./location": {
      "types": "./dist/location.d.ts",
      "import": "./dist/location.mjs",
      "require": "./dist/location.js"
    },
    "./logos": {
      "types": "./dist/logos.d.ts",
      "import": "./dist/logos.mjs",
      "require": "./dist/logos.js"
    },
    "./media": {
      "types": "./dist/media.d.ts",
      "import": "./dist/media.mjs",
      "require": "./dist/media.js"
    },
    "./navigation": {
      "types": "./dist/navigation.d.ts",
      "import": "./dist/navigation.mjs",
      "require": "./dist/navigation.js"
    },
    "./search": {
      "types": "./dist/search.d.ts",
      "import": "./dist/search.mjs",
      "require": "./dist/search.js"
    },
    "./social": {
      "types": "./dist/social.d.ts",
      "import": "./dist/social.mjs",
      "require": "./dist/social.js"
    },
    "./time": {
      "types": "./dist/time.d.ts",
      "import": "./dist/time.mjs",
      "require": "./dist/time.js"
    },
    "./ui-controls": {
      "types": "./dist/ui-controls.d.ts",
      "import": "./dist/ui-controls.mjs",
      "require": "./dist/ui-controls.js"
    },
    "./user": {
      "types": "./dist/user.d.ts",
      "import": "./dist/user.mjs",
      "require": "./dist/user.js"
    }
  },
  "license": "MIT",
  "repository": "",
  "private": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "docs": "npx typedoc --options ./typedoc.json",
    "prebuild": "node ../../source/update-badges.js",
    "release": "npm run test && npm run build && npm publish --access public",
    "build": "vite build",
    "dev": "vite build --watch",
    "start": "npm run dev",
    "pretest": "npm run pretest --prefix ../..",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:snapshot": "jest --updateSnapshot"
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^7.2.1",
    "typescript": "^5.8.2",
    "vite": "^7.1.4",
    "vite-plugin-checker": "^0.10.3",
    "vite-plugin-dts": "^4.5.4"
  }
}
