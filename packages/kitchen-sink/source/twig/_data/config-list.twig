{% set list = [
  {
    name: 'Spacing',
    link: 'spacing'
  },
  {
    name: 'Typography',
    link: 'typography'
  },
  {
    name: 'Kitchen Sink',
    link: 'kitchen-sink'
  }
] %}

{% macro DisplayName() %}
  {% for item in list %}
    {{ item.name }}
  {% endfor %}
{% endmacro %}

{% macro HeaderLinks() %}
  <ul>
    {% for item in list %}
      <li><a href="/configuration/{{ item.link }}">{{ item.name }}</a></li>
    {% endfor %}
  </ul>
{% endmacro %}

{% macro Links() %}
  {% for item in list %}
    <a href="/configuration/{{ item.link }}">{{ item.name }}</a>
  {% endfor %}
{% endmacro %}
