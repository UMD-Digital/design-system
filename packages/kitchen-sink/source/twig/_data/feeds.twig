{% set list = [
  {
    name: 'News Grid',
    link: 'news-grid'
  },
  {
    name: 'Events Grid',
    link: 'events-grid'
  },
  {
    name: 'Events List',
    link: 'events-list'
  }
] %}

{% macro DisplayName() %}
  {% for item in list %}
    {{ item.name }}
  {% endfor %}
{% endmacro %}

{% macro HeaderLinks() %}
  <ul>
    {% for item in list %}
      <li><a href="/feeds/{{ item.link }}">{{ item.name }}</a></li>
    {% endfor %}
  </ul>
{% endmacro %}

{% macro Links() %}
  {% for item in list %}
    <a href="/feeds/{{ item.link }}">{{ item.name }}</a>
  {% endfor %}
{% endmacro %}
