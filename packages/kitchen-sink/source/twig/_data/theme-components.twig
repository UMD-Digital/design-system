{% set list = [
  {
    name: 'Header',
    link: 'header'
  },
  {
    name: 'Sticky',
    link: 'sticky'
  }
] %}

{% macro DisplayName() %}
  {% for item in list %}
    {{ item.name }}
  {% endfor %}
{% endmacro %}

{% macro HeaderLinks() %}
  <ul>
    {% for item in list %}
      <li><a href="/theme/components/{{ item.link }}">{{ item.name }}</a></li>
    {% endfor %}
  </ul>
{% endmacro %}

{% macro Links() %}
  {% for item in list %}
    <a href="/theme/components/{{ item.link }}">{{ item.name }}</a>
  {% endfor %}
{% endmacro %}
