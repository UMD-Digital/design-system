{% set list = [
  {
    name: 'Root',
    link: 'root'
  },
  {
    name: 'Elements',
    link: 'elements'
  },
  {
    name: 'Watermark',
    link: 'watermark'
  }
] %}

{% macro DisplayName() %}
  {% for item in list %}
    {{ item.name }}
  {% endfor %}
{% endmacro %}

{% macro HeaderLinks() %}
  <ul>
    {% for item in list %}
      <li><a href="/theme/styles/{{ item.link }}">{{ item.name }}</a></li>
    {% endfor %}
  </ul>
{% endmacro %}

{% macro Links() %}
  {% for item in list %}
    <a href="/theme/styles/{{ item.link }}">{{ item.name }}</a>
  {% endfor %}
{% endmacro %}
