{% import '../_data/variable.twig' as VariableList %}
{% import '../_data/theme-styles.twig' as ThemeStylesList %}
{% import '../_data/theme-components.twig' as ThemeComponentsList %}
{% import '../_data/components.twig' as ComponenstList %}
{% import '../_data/feeds.twig' as FeedsList %}

<header>
  <div class="umd-lock">
    <nav style="display: flex; justify-content: flex-end;">
      <umd-element-nav-item>
        <a slot="primary-link" href="/"><span>Home</span></a>
      </umd-element-nav-item>

      <umd-element-nav-item>
        <a slot="primary-link" href="/variables"><span>Variables</span></a>
        <div slot="dropdown-links">
          {{ VariableList.HeaderLinks() }}
        </div>
      </umd-element-nav-item>

      <umd-element-nav-item>
        <a slot="primary-link" href="/theme/styles">
          <span>Theme Styles</span>
        </a>
        <div slot="dropdown-links">
          {{ ThemeStylesList.HeaderLinks() }}
        </div>
      </umd-element-nav-item>

      <umd-element-nav-item>
        <a slot="primary-link" href="/theme/components">
          <span>Theme Components</span>
        </a>
        <div slot="dropdown-links">
          {{ ThemeComponentsList.HeaderLinks() }}
        </div>
      </umd-element-nav-item>

      <umd-element-nav-item>
        <a slot="primary-link" href="/components">
          <span>Component Library</span>
        </a>
        <div slot="dropdown-links">
          {{ ComponenstList.HeaderLinks() }}
        </div>
      </umd-element-nav-item>

      <umd-element-nav-item>
        <a slot="primary-link" href="/feeds"><span>Feeds</span></a>
        <div slot="dropdown-links">
          {{ FeedsList.HeaderLinks() }}
        </div>
      </umd-element-nav-item>
    </nav>
  </div>
</header>
