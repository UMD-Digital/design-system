{% macro Eyebrow(params) %}
  {% set characters = params.characters ?? true %}

  {% set string = 'Optional Eyebrow Adipiscing Elite Iacute' %}

  {% if characters is defined and characters is not null and characters <= 44 %}
    {% set string = string|slice(0, characters) %}
  {% else %}
    {% set string = string|slice(0, 22) %}
  {% endif %}

  <div slot="eyebrow">
    <p>
      {{ string }}
    </p>
  </div>
{% endmacro %}

{% macro EyebrowString(params) %}
  {% set characters = params.characters ?? true %}

  {% set string = 'Optional Eyebrow Adipiscing Elite Iacute' %}

  {% if characters is defined and characters is not null and characters <= 80 %}
    {% set string = string|slice(0, characters) %}
  {% else %}
    {% set string = string|slice(0, 22) %}
  {% endif %}

  {{ string }}
{% endmacro %}

{% macro MinimalLandingHeroEyebrow(params) %}
