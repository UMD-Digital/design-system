{% extends '../_layout.twig' %}

{% block page %}
  <section style="min-height: 800px">
    <div class="umd-lock">
      <h2 class="umd-sans-extralarge">
        Default Example
      </h2>

      <umd-element-nav-drawer>
        <div slot="face">
          <a href="https://google.com" data-child-ref="slide-1">
            <span>Google</span>
          </a>

          <a href="https://github.com" data-child-ref="slide-2">
            <span>Github</span>
          </a>

          <a href="https://apple.com" data-child-ref="slide-3">
            <span>Apple</span>
          </a>
        </div>

        <div slot="slides">
          <div data-parent-ref="slide-1">
            <a href="https://docs.google.com/" data-child-ref="doc-slide-1">
              <span>Google Docs</span>
            </a>
            <a href="https://images.google.com/"><span>Google Images</span></a>
            <a href="https://maps.google.com/"><span>Google Maps</span></a>
          </div>

          <div data-parent-ref="doc-slide-1">
            <a href="https://www.youtube.com/watch?v=0k3RNZfMPZA">
              <span>Google Example</span>
            </a>
          </div>

          <div data-parent-ref="slide-3">
            <a href="https://www.apple.com/macbook-pro/">
              <span>Apple Macbook Pro</span>
            </a>
          </div>
        </div>
      </umd-element-nav-drawer>
    </div>
  </section>
{% endblock %}
