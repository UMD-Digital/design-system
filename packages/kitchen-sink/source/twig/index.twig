{% extends '_layout.twig' %}

{% import './sections/variables/_data.twig' as VariableList %}
{% import './sections/components/_data.twig' as ComponentsList %}

{% block page %}
  <section style="padding-bottom: 10vh">
    <div class="umd-lock">
      <h1 class="sr-only">
        No errors
      </h1>
      <h2 class="umd-sans-extralarge-uppercase" style="margin-bottom: 40px;">
        Variables
      </h2>
      <div class="umd-grid-gap-three">
        {{ VariableList.Links() }}
      </div>
    </div>
  </section>

  <section style="padding-bottom: 10vh">
    <div class="umd-lock">
      <h2 class="umd-sans-extralarge-uppercase" style="margin-bottom: 40px;">
        Components
      </h2>
      <div class="umd-grid-gap-three">
        {{ ComponentsList.Links() }}
      </div>
    </div>
  </section>
{% endblock %}
