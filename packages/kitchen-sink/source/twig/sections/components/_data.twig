] %}

{% macro GetUrl() %}
  {% set URL = '/sections/components/' %}
  {{ URL }}
{% endmacro %}

{% macro DisplayName() %}
  {% for item in list %}
    {{ item.name }}
  {% endfor %}
{% endmacro %}

{% macro HeaderLinks() %}
  <ul>
    {% for item in list %}
      <li><a href="{{ _self.GetUrl ~ item.link }}">{{ item.name }}</a></li>
    {% endfor %}
  </ul>
{% endmacro %}

{% macro Links() %}
  {% for item in list %}
    <a href="{{ _self.GetUrl ~ item.link }}">{{ item.name }}</a>
  {% endfor %}
{% endmacro %}
