{% extends '_layout.twig' %}

{% block page %}
  <section>
    <div class="umd-lock">
      <h1 class="page-heading">
        Feed Events Grid
      </h1>
    </div>

    <section>
      <div class="umd-lock">
        <h2 class="content-heading">
          No Results
        </h2>

        <umd-feed-events token="4zVU8phSIImj9Pb_XhvCLEj-OU5N6Rxw"
          row-count="2"
          show-count="4"
          categories="104"></umd-feed-events>
      </div>
    </section>

    <section style="padding-bottom: 40px">
      <div class="umd-lock">
        <h2 class="content-heading">
          <span>Multiple inclusions: Default Union</span>
          <span>Campus Leaders & Alumni Association</span>
        </h2>

        <umd-feed-events token="4zVU8phSIImj9Pb_XhvCLEj-OU5N6Rxw"
          categories="10256,10322"
          lazyload="true"
          show-count="3"></umd-feed-events>
      </div>
    </section>

    <section style="padding-bottom: 40px">
      <div class="umd-lock">
        <h2 class="content-heading">
          <span>Multiple inclusions: Intersection</span>
          <span>Campus Leaders & Alumni Association</span>
        </h2>

        <umd-feed-events token="4zVU8phSIImj9Pb_XhvCLEj-OU5N6Rxw"
          categories="10256,10322"
          lazyload="true"
          union="false"
          show-count="3"></umd-feed-events>
      </div>
    </section>

    <section style="padding: 50px 0; background-color: black;">
      <div class="umd-lock">
        <h2 class="content-heading">
          <span style="color: white;">Grid Two</span>
          <span style="color: white;">Lazy Load with Stamp Events</span>
        </h2>

        <umd-feed-events token="4zVU8phSIImj9Pb_XhvCLEj-OU5N6Rxw"
          categories="92369"
          lazyload="true"
          theme="dark"
          row-count="2"
          show-count="3"></umd-feed-events>
      </div>
    </section>
  </section>
{% endblock %}
