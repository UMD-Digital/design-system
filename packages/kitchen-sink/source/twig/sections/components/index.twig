{% extends '../../_layout.twig' %}

{% block page %}
  {% import './_data.twig' as List %}
  {% import './atomic/index.twig' as Atomic %}
  {% import './blocks/index.twig' as Blocks %}
  {% import './carousels/index.twig' as Carousels %}
  {% import './lists/index.twig' as Lists %}
  {% import './navigation/index.twig' as Navigation %}
  {% import './sliders/index.twig' as Sliders %}

  <section style="padding-bottom: 50px;">
    <div class="umd-lock">
      <h1 class="page-heading">
        Main Pages
      </h1>

      <div class="umd-grid-gap-four mb-md">
        {{ List.Links() }}
      </div>

      <hr />

      <h2 class="page-sub-heading">
        Sub Pages - <a href="{{ Atomic.GetChildUrl() }}">Atomic</a>
      </h2>

      <div class="umd-grid-gap-four mb-md">
        {{ Atomic.SubLinks() }}
      </div>

      <hr />

      <h2 class="page-sub-heading">
        Sub Pages - <a href="{{ Blocks.GetChildUrl() }}">Blocks</a>
      </h2>

      <div class="umd-grid-gap-four mb-md">
        {{ Blocks.SubLinks() }}
      </div>

      <hr />

      <h2 class="page-sub-heading">
        Sub Pages - <a href="{{ Carousels.GetChildUrl() }}">Carousels</a>
      </h2>

      <div class="umd-grid-gap-four mb-md">
        {{ Carousels.SubLinks() }}
      </div>

      <hr />

      <h2 class="page-sub-heading">
        Sub Pages - <a href="{{ Lists.GetChildUrl() }}">Lists</a>
      </h2>

      <div class="umd-grid-gap-four mb-md">
        {{ Lists.SubLinks() }}
      </div>

      <hr />

      <h2 class="page-sub-heading">
        Sub Pages - <a href="{{ Navigation.GetChildUrl() }}">Navigation</a>
      </h2>

      <div class="umd-grid-gap-four mb-md">
        {{ Navigation.SubLinks() }}
      </div>

      <hr />

      <h2 class="page-sub-heading">
        Sub Pages - <a href="{{ Sliders.GetChildUrl() }}">Sliders</a>
      </h2>

      <div class="umd-grid-gap-four mb-md">
        {{ Sliders.SubLinks() }}
      </div>
    </div>
  </section>
{% endblock %}
