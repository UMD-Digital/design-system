{% extends '../../_layout.twig' %}

{% block page %}
  {% import './_data.twig' as List %}

  <section>
    <div class="umd-lock">
      <h1 class="page-heading">
        Main Pages
      </h1>

      <div class="umd-grid-gap-four mb-md">
        {{ List.Links() }}
      </div>

      <hr />

      <h1 class="page-heading">
        Sub Pages
      </h1>

      <div class="umd-grid-gap-four mb-md">
        {{ List.SubLinks() }}
      </div>
    </div>
  </section>
{% endblock %}
