{% extends '../../_layout.twig' %}

{% block page %}
  {% import './_data.twig' as List %}

  <section style="padding-bottom: 50px;">
    <div class="umd-lock">
      <h1 class="page-heading">
        Main Pages
      </h1>

      <div class="umd-grid-gap-four mb-md">
        {{ List.Links() }}
      </div>

      <hr />

      <h2 class="page-sub-heading">
        Sub Pages - Blocks
      </h2>

      <div class="umd-grid-gap-four mb-md">
        {{ List.SubLinksBlock() }}
      </div>

      <hr />

      <h2 class="page-sub-heading">
        Sub Pages - Lists
      </h2>

      <div class="umd-grid-gap-four mb-md">
        {{ List.SubLinksList() }}
      </div>

      <hr />

      <h2 class="page-sub-heading">
        Sub Pages - Navigation
      </h2>

      <div class="umd-grid-gap-four mb-md">
        {{ List.SubLinksNavigation() }}
      </div>

      <hr />

      <h2 class="page-sub-heading">
        Sub Pages - Sliders
      </h2>

      <div class="umd-grid-gap-four mb-md">
        {{ List.SubLinksSlider() }}
      </div>
    </div>
  </section>
{% endblock %}
