    <section>
      <div class="umd-lock-smaller">
        <h2 class="content-heading">
          <span>Multiple inclusions: Intersection</span>
          <span>Events for Research and Arts & Culture</span>
        </h2>

        <umd-feed-news-list categories="10662, 10671"
          union="false"
          token="a3hwwnbL1aQtDZHr0-KhBC_XuyOZoG9l"></umd-feed-news-list>
      </div>
    </section>

    <section style="padding: 50px 0; background-color: black;">
      <div class="umd-lock-smaller">
        <h2 class="content-heading">
          <span style="color: white;">Lazy Load with Do Good Category</span>
          <span style="color: white;">Limit 10</span>
        </h2>

        <umd-feed-news-list token="a3hwwnbL1aQtDZHr0-KhBC_XuyOZoG9l"
          categories="10461"
          theme="dark"
          lazyload="true"
          row-count="10"></umd-feed-news-list>
      </div>
    </section>
  </section>
{% endblock %}
