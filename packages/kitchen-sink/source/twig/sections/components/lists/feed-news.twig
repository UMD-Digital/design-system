{% extends '_layout.twig' %}

{% block page %}
  <section>
    <div class="umd-lock-smaller">
      <h1 class="page-heading">
        Feed News List
      </h1>
    </div>

    <section>
      <div class="umd-lock-smaller">
        <h2 class="content-heading">
          No Results
        </h2>

        <umd-feed-news-list token="a3hwwnbL1aQtDZHr0-KhBC_XuyOZoG9l"
          row-count="2"
          show-count="4"
          categories="104"></umd-feed-news-list>
      </div>
    </section>

    <section>
      <div class="umd-lock-smaller">
        <h2 class="content-heading">
          <span>All Events</span>
          <span>Limit 5</span>
        </h2>

        <umd-feed-news-list token="a3hwwnbL1aQtDZHr0-KhBC_XuyOZoG9l"></umd-feed-news-list>
      </div>
