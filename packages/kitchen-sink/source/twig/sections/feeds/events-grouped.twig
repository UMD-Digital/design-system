{% extends '../../_layout.twig' %}

{% block page %}
  <section>
    <div class="umd-lock">
      <h1 class="page-heading">
        Feed Events Grouped
      </h1>
    </div>

    <section>
      <div class="umd-lock">
        <h2 class="content-heading">
          <span>All Events</span>
          <span>Limit 10</span>
        </h2>

        <div style="max-width: 800px; margin: 50px auto;">
          <umd-feed-events-grouped lazyload="true"
            token="4zVU8phSIImj9Pb_XhvCLEj-OU5N6Rxw"></umd-feed-events-grouped>
        </div>
      </div>
    </section>

    <section style="padding: 50px 0; background-color: black;">
      <div class="umd-lock">
        <h2 class="content-heading">
          <span style="color: white;">All Events (Dark Background)</span>
          <span style="color: white;">Limit 10</span>
        </h2>

        <div style="max-width: 800px; margin: 50px auto;">
          <umd-feed-events-grouped lazyload="true"
            theme="dark"
            token="4zVU8phSIImj9Pb_XhvCLEj-OU5N6Rxw"></umd-feed-events-grouped>
        </div>
      </div>
    </section>
  </section>
{% endblock %}
