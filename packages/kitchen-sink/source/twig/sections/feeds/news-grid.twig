{% extends '../../_layout.twig' %}

{% block page %}
  <section>
    <div class="umd-lock">
      <h1 class="page-heading">
        Feed News Grid
      </h1>
    </div>

    <section>
      <div class="umd-lock">
        <h2 class="content-heading type-hr">
          No Results
        </h2>

        <umd-feed-news token="a3hwwnbL1aQtDZHr0-KhBC_XuyOZoG9l"
          row-count="2"
          show-count="4"
          categories="104"></umd-feed-news>
      </div>
    </section>

    <section style="padding-bottom: 40px">
      <div class="umd-lock">
        <h2 class="content-heading type-hr">
          <span>Multiple inclusions (default union)</span>
          <span>Events for Research and Arts & Culture</span>
        </h2>

        <umd-feed-news token="a3hwwnbL1aQtDZHr0-KhBC_XuyOZoG9l"
          categories="10662, 10671"
          lazyload="true"
          show-count="3"></umd-feed-news>
      </div>
    </section>

    <section style="padding-bottom: 40px">
      <div class="umd-lock">
        <h2 class="content-heading type-hr">
          <span>Multiple inclusions: Intersection</span>
          <span>Events for Research and Arts & Culture</span>
        </h2>

        <umd-feed-news token="a3hwwnbL1aQtDZHr0-KhBC_XuyOZoG9l"
          categories="10662, 10671"
          union="false"
          lazyload="true"
          show-count="3"></umd-feed-news>
      </div>
    </section>

    <section style="padding: 50px 0; background-color: black;">
      <div class="umd-lock">
        <h2 class="content-heading type-hr">
          <span style="color: white">Grid Three (dark)</span>
          <span style="color: white">Lazy Load with Do Good Category</span>
        </h2>

        <umd-feed-news token="a3hwwnbL1aQtDZHr0-KhBC_XuyOZoG9l"
          theme="dark"
          categories="10461"
          lazyload="true"
          row-count="2"
          show-count="3"></umd-feed-news>
      </div>
    </section>
  </section>
{% endblock %}
