{% set list = [
  {
    name: 'Terp Home',
    link: 'terp-home'
  },
  {
    name: 'Terp Interior',
    link: 'terp-interior'
  },
  {
    name: 'Greater CP Home',
    link: 'greater-home'
  },
  {
    name: 'Greater CP About',
    link: 'greater-about'
  },
  {
    name: 'Greater CP Amenities',
    link: 'greater-amenities'
  },
  {
    name: 'Greater CP Community',
    link: 'greater-community'
  },
  {
    name: 'Greater CP Discovery',
    link: 'greater-discovery'
  },
  {
    name: 'Greater CP Flex Research',
    link: 'greater-flex-research'
  }
] %}

{% macro GetUrl() %}
  {% set URL = '/sections/templates/' %}
  {{ URL }}
{% endmacro %}

{% macro DisplayName() %}
  {% for item in list %}
    {{ item.name }}
  {% endfor %}
{% endmacro %}

{% macro HeaderLinks() %}
  <ul>
    {% for item in list %}
      <li><a href="{{ _self.GetUrl ~ item.link }}">{{ item.name }}</a></li>
    {% endfor %}
  </ul>
{% endmacro %}

{% macro Links() %}
  {% for item in list %}
    <a href="{{ _self.GetUrl ~ item.link }}">{{ item.name }}</a>
  {% endfor %}
{% endmacro %}
