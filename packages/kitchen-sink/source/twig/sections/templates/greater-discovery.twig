{% extends './layouts/greater-cp.twig' %}

{% block page %}
  <umd-element-hero type="default-centered" class="umd-layout-vertical-landing">
    <img slot="image"
      src="/greater-cp/discovery-hero.jpg"
      alt="Image Description" />

    <p slot="headline">
      DISCOVERY DISTRICT
    </p>
    <p slot="text">
      Our dynamic research park brings together a vibrant and inclusive
      community of university stakeholders, corporate partners, government
      researchers, entrpreneurs, and local residents.
    </p>
  </umd-element-hero>

  <section class="umd-layout-vertical-landing">
    <div class="umd-lock-extra-small">
      <umd-element-section-intro include-separator
        class="umd-layout-vertical-landing-child">
        <h2 slot="text">
          We are activating fearless ideas for transformational impact by
          fostering research and industry partnerships, supporting small
          businesses, and nurturing the growth of next generation technology
          ventures.
        </h2>
        <umd-element-call-to-action type="primary" slot="actions">
          <a href="/"><span>Learn more</span></a>
        </umd-element-call-to-action>
      </umd-element-section-intro>
    </div>
  </section>

  <section class="umd-layout-vertical-landing">
    <div class="umd-lock">
      <div class="umd-grid-gap-three">
        <umd-element-stat size="large" has-line>
          <div slot="stat">
            60+
          </div>
          <div slot="text">
            Companies, Organizations, and Four Federal Agencies
          </div>
        </umd-element-stat>

        <umd-element-stat size="large" has-line>
          <div slot="stat">
            6,700+
          </div>
          <div slot="text">
            Community Members
          </div>
        </umd-element-stat>

        <umd-element-stat size="large" has-line>
          <div slot="stat">
            2M+
          </div>
          <div slot="text">
            Square Feet of Office, Retail, Residential, and Research Space
          </div>
        </umd-element-stat>
      </div>
    </div>
  </section>

  <section class="umd-layout-vertical-landing">
    <umd-element-pathway image-scaled image-position="left">
      <div slot="image">
        <img src="/greater-cp/discovery-pathway-1.jpg" alt="Image" />
      </div>

      <p slot="headline">
        Home of the National Quantum Lab (QLAB)
      </p>

      <div slot="text">
        <p>
          <a target="_blank" rel="noopener" href="https://ionq.com/">
            IonQ, Inc. (IonQ)
          </a>, the leading developer of quantum computing devices, and the
          Universiyt of Maryland (UMD), an international powerhouse in quantum
          research and applications, have launched the National Quantum Lab at
          Maryland (QLab) and commemorated the grand opening with a <a target="_blank"
            rel="noopener"
            href="https://today.umd.edu/umd-celebrates-grand-opening-of-quantum-computing-research-hub">
            ribbon cutting ceremony
          </a>.
        </p>
      </div>
      <div slot="actions">
        <umd-element-call-to-action type="secondary">
          <a href="https://qlab.umd.edu/" aria-label="Read about the new QLab">
            <svg aria-hidden="true"
              width="96"
              height="96"
              viewBox="0 0 96 96"
              fill="none"
              xmlns="http://www.w3.org/2000/svg">
              <path fill-rule="evenodd"
                clip-rule="evenodd"
                d="M39.9149 15.7825L67.7095 15.7653L58.5275 25.1687H2V87.9013H65.8332V33.8977L75.175 24.3307L75.1772 50.9945L86.9541 38.8741L86.9553 15.7534L87 15.7534L87 4.00001L86.9559 4.0001L75.1733 4.0001L52.0353 4.00001L39.9149 15.7825ZM49.1034 34.8198L33.3385 50.9648L41.3473 58.9736L56.0127 43.9548V78.2501H11.8205V34.8198H49.1034Z" />
            </svg>Read more about the new QLab
          </a>
        </umd-element-call-to-action>
      </div>
    </umd-element-pathway>
  </section>

  <section class="umd-layout-vertical-landing">
    <div class="umd-lock">
      <div class="umd-grid-gap-three">
        <umd-element-stat size="large" has-line>
          <div slot="stat">
            150
          </div>
          <div slot="text">
            Acres comprise the research park adjacent to the University of
            Maryland
          </div>
        </umd-element-stat>

        <umd-element-stat size="large" has-line>
          <div slot="stat">
            297
          </div>
          <div slot="text">
            Hotel Rooms and 12 Restaurants
          </div>
        </umd-element-stat>

        <umd-element-stat size="large" has-line>
          <div slot="stat">
            162
          </div>
          <div slot="text">
            Acres of nearby Trails and Parks
          </div>
        </umd-element-stat>
      </div>
    </div>
  </section>

  <section class="umd-layout-vertical-landing">
    <div class="umd-lock">
      <p>
        Hero Person To Come
      </p>
    </div>
  </section>

  <section class="umd-layout-vertical-landing">
    <umd-element-pathway isimagefirst="false">
      <div slot="image">
        <img src="/greater-cp/discovery-pathway-2.jpg" alt="Image" />
      </div>

      <p slot="headline">
        Meet our Community Members
      </p>

      <div slot="text">
        <p>
          The state's flagship university and one of the nation's preeminent
          public research universities, UMD is a global leader in research,
          entrepreneurship and innovation. The university is home to more than
          40,700 students, 14,000 faculty and staff, and 300 academic programs.
        </p>
      </div>
      <div slot="actions">
        <umd-element-call-to-action type="secondary">
          <a href="https://google.com" aria-label="link to google">
            Community Directory
          </a>
        </umd-element-call-to-action>
      </div>
    </umd-element-pathway>
  </section>

  <section class="umd-layout-vertical-landing">
    <umd-element-pathway type="overlay"
      theme="dark"
      image-scaled
      image-position="left">
      <div slot="image">
        <img src="/greater-cp/discovery-pathway-4.jpg" alt="Image" />
      </div>

      <p slot="headline">
        Space for Innovation
      </p>

      <div slot="text">
        <p>
          The Discovery District offers flexible space options from incubator
          space for early stage startups to flexible research and development
          buildings for growing ventures, as well as customizable options for
          larger companies.
        </p>
      </div>
      <div slot="actions">
        <umd-element-call-to-action type="secondary" theme="dark">
          <a href="https://qlab.umd.edu/" aria-label="Read about the new QLab">
            Real Estate
          </a>
        </umd-element-call-to-action>
      </div>
    </umd-element-pathway>

    <umd-element-pathway type="overlay"
      theme="light"
      image-scaled
      image-position="left">
      <div slot="image">
        <img src="/greater-cp/discovery-pathway-5.jpg" alt="Image" />
      </div>

      <p slot="headline">
        More than a Place to Work
      </p>

      <div slot="text">
        <p>
          More than plentiful office and research space, the Discovery District
          neighborhood is a vibrant place with opportunities to enjoy
          recreation, a great meal, and places to rest and relax.
        </p>
      </div>
      <div slot="actions">
        <umd-element-call-to-action type="secondary">
          <a href="https://qlab.umd.edu/" aria-label="Read about the new QLab">
            Ammenities & Attractions
          </a>
        </umd-element-call-to-action>
      </div>
    </umd-element-pathway>

    <umd-element-pathway type="overlay"
      theme="light"
      image-scaled
      image-position="left">
      <div slot="image">
        <img src="/greater-cp/discovery-pathway-6.jpg" alt="Image" />
      </div>

      <p slot="headline">
        We Would Love to Show you Around
      </p>

      <div slot="text">
        <p>
          To discuss space for your organization in the Discovery District,
          arrange a tour, or to learn more information, please contact us.
        </p>
      </div>
      <div slot="actions">
        <umd-element-call-to-action type="secondary">
          <a href="https://qlab.umd.edu/" aria-label="Read about the new QLab">
            Contact Us
          </a>
        </umd-element-call-to-action>
      </div>
    </umd-element-pathway>
  </section>

  <section class="umd-layout-vertical-landing">
    <umd-element-pathway isimagefirst="false">
      <div slot="image">
        <img src="/greater-cp/discovery-pathway-3.jpg" alt="Image" />
      </div>

      <p slot="headline">
        Stay Connected
      </p>

      <div slot="text">
        <p>
          Keep up-to-date on the latest news, research breakthroughs, events,
          and employment opportunities in the Discovery District
        </p>
        <ul>
          <li>Read the latest newsletter</li>
          <li>Join the Linkedin Group</li>
          <li>Follow Us on X (formerly Twitter)</li>
        </ul>
      </div>

      <div slot="actions">
        <umd-element-call-to-action type="secondary">
          <a href="https://google.com" aria-label="link to google">
            Subscribe to the newsletter
          </a>
        </umd-element-call-to-action>
      </div>
    </umd-element-pathway>
  </section>

  {% include './components/footer-greater-cp.twig' with {
    includeVisual: true
  } %}
{% endblock %}
