{% set list = [
  {
    name: 'Animations',
    link: 'animations'
  },
  {
    name: 'Elements',
    link: 'elements'
  },
  {
    name: 'Rich Text',
    link: 'rich-text'
  },
  {
    name: 'Text',
    link: 'text'
  },
  {
    name: 'Typography',
    link: 'typography'
  }
] %}

{% macro GetUrl() %}
  {% set URL = '/sections/theme/' %}
  {{ URL }}
{% endmacro %}

{% macro DisplayName() %}
  {% for item in list %}
    {{ item.name }}
  {% endfor %}
{% endmacro %}

{% macro HeaderLinks() %}
  <ul>
    {% for item in list %}
      <li><a href="{{ _self.GetUrl ~ item.link }}">{{ item.name }}</a></li>
    {% endfor %}
  </ul>
{% endmacro %}

{% macro Links() %}
  {% for item in list %}
    <a href="{{ _self.GetUrl ~ item.link }}">{{ item.name }}</a>
  {% endfor %}
{% endmacro %}
