{% set list = [
  {
    name: 'Tokens',
    link: 'tokens'
  },
  {
    name: 'Root',
    link: 'root'
  },
  {
    name: 'Delete Me - Header',
    link: 'header'
  },
  {
    name: 'Delete Me - Section Intro',
    link: 'section-intro'
  },
  {
    name: 'Delete Me - Sticky',
    link: 'sticky'
  }
] %}

{% macro GetUrl() %}
  {% set URL = '/sections/variables/' %}
  {{ URL }}
{% endmacro %}

{% macro DisplayName() %}
  {% for item in list %}
    {{ item.name }}
  {% endfor %}
{% endmacro %}

{% macro HeaderLinks() %}
  <ul>
    {% for item in list %}
      <li><a href="{{ _self.GetUrl ~ item.link }}">{{ item.name }}</a></li>
    {% endfor %}
  </ul>
{% endmacro %}

{% macro Links() %}
  {% for item in list %}
    <a href="{{ _self.GetUrl ~ item.link }}">{{ item.name }}</a>
  {% endfor %}
{% endmacro %}
