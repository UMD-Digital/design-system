{
  "name": "@universityofmaryland/web-styles-library",
  "version": "1.8.1",
  "description": "Styles used for use in the University of Maryland Design System",
  "type": "module",
  "main": "dist/index.js",
  "author": "UMD Web Services <digital@umd.edu>",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./accessibility": {
      "types": "./dist/accessibility.d.ts",
      "import": "./dist/accessibility.js",
      "default": "./dist/accessibility.js"
    },
    "./animation": {
      "types": "./dist/animation.d.ts",
      "import": "./dist/animation.js",
      "default": "./dist/animation.js"
    },
    "./element": {
      "types": "./dist/element.d.ts",
      "import": "./dist/element.js",
      "default": "./dist/element.js"
    },
    "./layout": {
      "types": "./dist/layout.d.ts",
      "import": "./dist/layout.js",
      "default": "./dist/layout.js"
    },
    "./typography": {
      "types": "./dist/typography.d.ts",
      "import": "./dist/typography.js",
      "default": "./dist/typography.js"
    },
    "./utilities": {
      "types": "./dist/utilities.d.ts",
      "import": "./dist/utilities.js",
      "default": "./dist/utilities.js"
    },
    "./tailwind": {
      "types": "./dist/tailwind.d.ts",
      "import": "./dist/tailwind.js",
      "default": "./dist/tailwind.js"
    },
    "./tailwind.css": {
      "style": "./dist/tailwind.css",
      "import": "./dist/tailwind.css",
      "default": "./dist/tailwind.css"
    },
    "./tailwind-theme.css": {
      "style": "./dist/tailwind-theme.css",
      "import": "./dist/tailwind-theme.css",
      "default": "./dist/tailwind-theme.css"
    },
    "./css/styles.min.css": {
      "style": "./dist/css/styles.min.css",
      "import": "./dist/css/styles.min.css",
      "default": "./dist/css/styles.min.css"
    },
    "./css/tokens.min.css": {
      "style": "./dist/css/tokens.min.css",
      "import": "./dist/css/tokens.min.css",
      "default": "./dist/css/tokens.min.css"
    },
    "./css/base.min.css": {
      "style": "./dist/css/base.min.css",
      "import": "./dist/css/base.min.css",
      "default": "./dist/css/base.min.css"
    },
    "./css/typography.min.css": {
      "style": "./dist/css/typography.min.css",
      "import": "./dist/css/typography.min.css",
      "default": "./dist/css/typography.min.css"
    },
    "./css/layout.min.css": {
      "style": "./dist/css/layout.min.css",
      "import": "./dist/css/layout.min.css",
      "default": "./dist/css/layout.min.css"
    },
    "./css/element.min.css": {
      "style": "./dist/css/element.min.css",
      "import": "./dist/css/element.min.css",
      "default": "./dist/css/element.min.css"
    },
    "./css/animation.min.css": {
      "style": "./dist/css/animation.min.css",
      "import": "./dist/css/animation.min.css",
      "default": "./dist/css/animation.min.css"
    },
    "./css/accessibility.min.css": {
      "style": "./dist/css/accessibility.min.css",
      "import": "./dist/css/accessibility.min.css",
      "default": "./dist/css/accessibility.min.css"
    },
    "./css/web-components.min.css": {
      "style": "./dist/css/web-components.min.css",
      "import": "./dist/css/web-components.min.css",
      "default": "./dist/css/web-components.min.css"
    },
    "./exports/generate": {
      "types": "./dist/exports/generate.d.ts",
      "import": "./dist/exports/generate.js",
      "default": "./dist/exports/generate.js"
    }
  },
  "license": "MIT",
  "repository": "",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "docs": "npx typedoc --options ./typedoc.json",
    "clean": "rm -rf dist",
    "prebuild": "node ../../scripts/update-badges.js",
    "release": "npm run test && npm run build && npm publish --access public",
    "build": "vite build && npm run build:cdn && npm run build:css",
    "build:cdn": "BUILD_CDN=true vite build --mode production",
    "build:css": "tsx scripts/generate-css.ts",
    "build:tailwind": "tsx scripts/generate-tailwind.ts",
    "dev": "vite build --watch",
    "start": "npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:snapshot": "jest --updateSnapshot"
  },
  "dependencies": {
    "@universityofmaryland/web-token-library": "^1.0.0",
    "postcss": "^8.5.6",
    "postcss-discard-duplicates": "^7.0.1",
    "postcss-js": "^4.0.1",
    "postcss-nesting": "^13.0.1"
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^7.2.1",
    "typescript": "^5.8.2",
    "vite": "^7.1.4",
    "vite-plugin-checker": "^0.10.3",
    "vite-plugin-dts": "^4.5.4"
  }
}
