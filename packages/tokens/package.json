{
  "name": "@universityofmaryland/web-token-library",
  "version": "1.0.1-beta.0",
  "description": "Design tokens for the University of Maryland Design System - Foundation for Figma-to-code pipeline",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./color": {
      "types": "./dist/color.d.ts",
      "import": "./dist/color.js",
      "default": "./dist/color.js"
    },
    "./font": {
      "types": "./dist/font.d.ts",
      "import": "./dist/font.js",
      "default": "./dist/font.js"
    },
    "./media": {
      "types": "./dist/media.d.ts",
      "import": "./dist/media.js",
      "default": "./dist/media.js"
    },
    "./spacing": {
      "types": "./dist/spacing.d.ts",
      "import": "./dist/spacing.js",
      "default": "./dist/spacing.js"
    }
  },
  "license": "MIT",
  "repository": "",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "docs": "npx typedoc --options ./typedoc.json",
    "prebuild": "node ../../scripts/update-badges.js",
    "release": "npm run test && npm run build && npm publish --access public",
    "build": "vite build && npm run build:cdn",
    "build:cdn": "BUILD_CDN=true vite build --mode production",
    "dev": "vite build --watch",
    "start": "npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:snapshot": "jest --updateSnapshot"
  },
  "keywords": [
    "design-tokens",
    "design-system",
    "umd",
    "university-of-maryland",
    "figma"
  ],
  "devDependencies": {
    "@vitejs/plugin-legacy": "^7.2.1",
    "typescript": "^5.8.2",
    "vite": "^7.1.4",
    "vite-plugin-checker": "^0.10.3",
    "vite-plugin-dts": "^4.5.4"
  }
}
