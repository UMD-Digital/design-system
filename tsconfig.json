{
  "compilerOptions": {
    "strict": true,
    "module": "ESNext",
    "esModuleInterop": true,
    "lib": ["es6", "dom", "dom.iterable"],
    "target": "ES2020",
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "removeComments": true,
    "forceConsistentCasingInFileNames": true,
  }
}
